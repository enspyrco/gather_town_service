    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.42",
    "@types/ws": "^8.5.3",
    "chai": "^4.3.6",
    "google-auth-library": "^7.0.0",
    "got": "^11.5.0",
    "mocha": "^9.0.0",
    "nodemon": "^2.0.16",
    "sinon": "^14.0.0",
    "supertest": "^6.0.0",
